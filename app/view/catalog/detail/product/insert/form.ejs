<form id="product-insert-form" class="box b1 container border-st radius-5 padding-5 margin-top-10"
  style="display: none;">
  <div class="box b1 underline lucida-grande em08 bold center">Adicionar produto ao cat√°logo</div>
  <input type="hidden" name="catalog_id" value="">
  <%- include('./dropdown/input.ejs') %>
  <input type="number" name="price" class="box b6 input-generic lucida-grande bold padding-5 margin-top-5 nofocus"
    step="0.01" value="0.00" onfocus="if(this.value < 0.01){this.value=''}"
    onblur="if(this.value < 0.01){ this.value=parseFloat(0).toFixed(2) } else { this.value=parseFloat(this.value).toFixed(2) }"
    autocomplete="off" role="presentation" />
  <select name="status" class="box b6 input-generic lucida-grande bold padding-5 margin-top-5">
    <option value="Ativo">Ativo</option>
    <option value="Inativo">Inativo</option>
  </select>
  <input type="submit" name="submit" class="box b6 input-generic btn-act bold noborder padding-5 margin-top-5 pointer"
    value="Adicionar">
</form>